@prefix prov <https://www.w3.org/ns/prov#>

prov.Entity : owl.Class()
  dc.description = "A physical, digital, conceptual, or other kind of thing with some fixed aspects; entities may be real or imaginary."

prov.Activity : owl.Class()
  dc.description = "Something that occurs over a period of time and acts upon or with entities."

prov.Agent : owl.Class()
  dc.description = "Something that bears some form of responsibility for an activity taking place."

